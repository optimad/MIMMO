<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- xml interface: testing stitching between geometries -->
<mimmoXML version="1">
    <Blocks>
        <Geom0>
            <ClassName>mimmo.Geometry</ClassName>
            <IOMode>CONVERT</IOMode>
            <ReadDir>./geodata</ReadDir>
            <ReadFilename>sphere2</ReadFilename>
            <ReadFileType>STL</ReadFileType>
            <WriteDir>./</WriteDir>
            <WriteFilename>geohandlers_output_00002.0000</WriteFilename>
            <WriteFileType>STL</WriteFileType>
        </Geom0>
        <Geom1>
            <ClassName>mimmo.Geometry</ClassName>
            <IOMode>CONVERT</IOMode>
            <ReadDir>./geodata</ReadDir>
            <ReadFilename>stanfordBunny2</ReadFilename>
            <ReadFileType>STL</ReadFileType>
            <WriteDir>./</WriteDir>
            <WriteFilename>geohandlers_output_00002.0001</WriteFilename>
            <WriteFileType>STL</WriteFileType>
        </Geom1>
        <Geom2>
            <ClassName>mimmo.Geometry</ClassName>
            <IOMode>WRITE</IOMode>
            <WriteDir>./</WriteDir>
            <WriteFilename>geohandlers_output_00002.0002</WriteFilename>
            <WriteFileType>STL</WriteFileType>
        </Geom2>
        <Stitch>
            <ClassName>mimmo.StitchGeometry</ClassName>
        </Stitch>
    </Blocks>
	<Connections>
		<c0>
			<sender>Geom0</sender>
			<senderPort>M_GEOM</senderPort>
			<receiver>Stitch</receiver>
			<receiverPort>M_GEOM</receiverPort>
		</c0>
		<c1>
			<sender>Geom1</sender>
			<senderPort>M_GEOM</senderPort>
			<receiver>Stitch</receiver>
			<receiverPort>M_GEOM</receiverPort>
		</c1>
		<c2>
			<sender>Stitch</sender>
			<senderPort>M_GEOM</senderPort>
			<receiver>Geom2</receiver>
			<receiverPort>M_GEOM</receiverPort>
		</c2>
	</Connections>
</mimmoXML>
