FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application         simpleFoam;
startFrom           startTime;
startTime           0;
stopAt              endTime;
endTime             4000;
deltaT              1;
writeControl        runTime;
writeInterval       4000;
purgeWrite          0;
writeFormat         binary;
writePrecision      10;
writeCompression    off;
timeFormat          general;
timePrecision       10;
runTimeModifiable   true;

functions
{
}

